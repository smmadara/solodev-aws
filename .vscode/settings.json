{
    "deploy.reloaded": {
        "deployOnSave": true,
        "packages": [
            {
                "name": "Solodev AWS",
                "description": "",
                "files": [
                    "aws/**",
                ],
                "exclude": [
                    "tests/**"
                ]
            }
        ],
        "targets": [
            {
                "type": "s3bucket",
                "name": "solodev-aws-ha",
                "description": "AWS S3 bucket",
                "bucket": "solodev-aws-ha",
                "credentials":{
                    "config": "/inetpub/solodev-aws/.vscode/credentials.json",
                    "type": "file"
                }
            }
        ]
    }
}